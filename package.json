{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "animate.css": "^4.1.1",
    "gatsby": "^3.8.1",
    "gatsby-image": "^3.8.0",
    "gatsby-plugin-anchor-links": "^1.2.1",
    "gatsby-plugin-google-analytics": "^3.8.0",
    "gatsby-plugin-manifest": "^3.8.0",
    "gatsby-plugin-offline": "^4.8.0",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^4.8.0",
    "gatsby-plugin-react-redux": "^1.1.0",
    "gatsby-plugin-sass": "^4.8.0",
    "gatsby-plugin-sharp": "^3.8.0",
    "gatsby-source-filesystem": "^3.8.0",
    "gatsby-source-wordpress": "^5.8.0",
    "gatsby-transformer-sharp": "^3.8.0",
    "lint-staged": "^11.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "node-sass": "^6.0.1",
    "og-css": "^3.1.4",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-html-parser": "^2.0.2",
    "react-redux": "^7.2.4",
    "react-truncate-markup": "^5.1.0",
    "react-visibility-sensor": "^5.1.1",
    "redux": "^4.1.0",
    "redux-persist": "^6.0.0",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0"
  },
  "devDependencies": {
    "eslint-config-react-app": "^6.0.0",
    "gatsby-plugin-root-import": "^2.0.6",
    "husky": "^7.0.0",
    "prettier": "^2.3.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "precommit": "cross-env NODE_ENV=production lint-staged",
    "lint": "eslint --ext .js,.vue src ",
    "lint-format": "prettier --check \"**/*.{js,jsx,json,md}\"",
    "clean": "gatsby clean",
    "test": "npm run lint & npm run lint-format"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md}": [
      "prettier --write"
    ]
  }
}
